mixin atmosphere-select(data)
  .atmosphere-select
    .atmosphere-select-content
      .select
        .select__label атмосфера 
          span для
        .select-item 
          .select-item__title выбрать
          .select-item-list
            for item in select
              a(href='club.html#' + '#{item.link}' title=item.title target="_blank").select-item-list__item #{item.title}
  .mask